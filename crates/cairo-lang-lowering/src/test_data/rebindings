//! > ==========================================================================

//! > Test rebindings with early return.

//! > test_function_name
test_function_lowering

//! > function
fn foo(ref a: felt, b: felt, c: felt) -> felt {
    a = b;
    a = bar()
    if b == 2 {
      a = c;
      return b;
    }
    c
}

//! > function_name
foo

//! > module_code
